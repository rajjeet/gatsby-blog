{
  "name": "ortmesh-blog",
  "description": "A blog by Rajjeet Phull on software development",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf .cache",
    "dev": "gatsby develop",
    "start": "npm run dev",
    "build": "npm run clean && gatsby build",
    "start:prod": "npm run build && gatsby serve",
    "deploy:aws-s3": "aws s3 sync public/ s3://ortmesh.com --acl public-read --delete --exclude CloudFrontLogs*",
    "deploy": "npm run test:e2e && npm run deploy:aws-s3",
    "lint": "eslint '**/*.{js,jsx}'",
    "lint:fix": "eslint --fix '**/*.{js,jsx}'",
    "test": "jest",
    "test:watch": "jest --watch",
    "storybook": "NODE_ENV=production start-storybook -p 6006 -s public",
    "build-storybook": "NODE_ENV=production build-storybook -s public",
    "run:codeceptjs": "npx codeceptjs run",
    "test:e2e": "start-server-and-test start:prod http://localhost:9000 run:codeceptjs"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run test",
      "pre-push": "npm run test:e2e"
    }
  },
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.0",
    "gatsby": "^2.1.2",
    "styled-components": "^4.3.2",
    "prismjs": "^1.17.1",
    "tocbot": "^4.8.0",
    "clipboard": "^2.0.4",
    "@fortawesome/fontawesome-svg-core": "^1.2.18",
    "@fortawesome/free-brands-svg-icons": "^5.8.2",
    "@fortawesome/free-solid-svg-icons": "^5.8.2",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@mdx-js/mdx": "^1.4.4",
    "@mdx-js/react": "^1.4.4",
    "disqus-react": "^1.0.5",
    "lodash": "^4.17.11"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "devDependencies": {
    "gatsby-image": "^2.0.29",
    "gatsby-source-filesystem": "^2.0.20",
    "gatsby-transformer-remark": "^2.2.5",
    "gatsby-transformer-sharp": "^2.2.13",
    "gatsby-plugin-google-analytics": "^2.0.20",
    "gatsby-plugin-manifest": "^2.0.17",
    "gatsby-plugin-mdx": "^1.0.39",
    "gatsby-plugin-offline": "^2.0.23",
    "gatsby-plugin-react-helmet": "^3.0.6",
    "gatsby-plugin-sharp": "^2.2.19",
    "gatsby-plugin-styled-components": "^3.0.7",
    "gatsby-plugin-webpack-size": "^1.0.0",
    "gatsby-remark-images": "^3.0.4",
    "@babel/core": "^7.5.5",
    "@storybook/addon-actions": "^5.1.11",
    "@storybook/addon-links": "^5.1.11",
    "@storybook/addons": "^5.1.11",
    "@storybook/addon-knobs": "^5.1.11",
    "@storybook/react": "^5.1.11",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "babel-plugin-remove-graphql-queries": "^2.7.5",
    "babel-preset-gatsby": "^0.2.11",
    "babel-jest": "^24.9.0",
    "babel-plugin-prismjs": "^1.1.1",
    "babel-plugin-styled-components": "^1.10.0",
    "eslint": "^6.2.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-codeceptjs": "^1.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.15.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "jest": "^24.9.0",
    "@testing-library/jest-dom": "^4.1.0",
    "@testing-library/react": "^9.1.3",
    "@types/jest": "^24.0.18",
    "codeceptjs": "^2.3.0",
    "husky": "^3.0.4",
    "start-server-and-test": "^1.10.0",
    "puppeteer": "^1.19.0",
    "identity-obj-proxy": "^3.0.0",
    "url-join": "^4.0.1",
    "lodash-webpack-plugin": "^0.11.5"
  }
}
